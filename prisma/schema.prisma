// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int      @id @default(autoincrement())
  email    String   @unique
  password String  
  name     String?
  created  DateTime?
  isActive Boolean
}

model Session {
  id          Int   @id @default(autoincrement())
  user_id     Int
  active      Boolean?  
  date        DateTime?
  fingerprint String?
}

model UserStorage {
  id      Int       @id @default(autoincrement())
  data    Json?
}